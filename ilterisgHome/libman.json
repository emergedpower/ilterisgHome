{
  "version": "3.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "aos@2.3.1",
      "destination": "wwwroot/lib/aos",
      "files": [
        "dist/aos.js",
        "dist/aos.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "slick-carousel@1.8.1",
      "destination": "wwwroot/lib/slick",
      "files": [
        "slick/slick.min.js",
        "slick/slick.css",
        "slick/slick-theme.css"
      ]
    }
  ]
}